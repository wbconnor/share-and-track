<p id="notice"><%= notice %></p>

<h1>Achievements</h1>

<table>
  <thead>
    <tr>
      <th>Activity</th>
      <th>Session</th>
      <th>Quantity</th>
      <th>Weight</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @achievements.each do |achievement| %>
      <tr>
        <td><%= achievement.activity_id %></td>
        <td><%= achievement.session_id %></td>
        <td><%= achievement.quantity %></td>
        <td><%= achievement.weight %></td>
        <td><%= achievement.time %></td>
        <td><%= link_to 'Show', achievement %></td>
        <td><%= link_to 'Edit', edit_achievement_path(achievement) %></td>
        <td><%= link_to 'Destroy', achievement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Achievement', new_achievement_path %>
